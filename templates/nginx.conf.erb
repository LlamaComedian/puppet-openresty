worker_processes  <%= @worker_processes %>;

pid /opt/openresty/log/nginx.pid;
error_log <%= @nginx_base_dir %>/logs/error.log info;


events {
    worker_connections  <%= @worker_connections %>;
}

http {
    variables_hash_max_size 1024;
    access_log <%= @nginx_base_dir %>/logs/access.log;
    include    <%= @nginx_base_dir %>/nginx/conf/mime.types;
    charset utf-8;

    include <%= @nginx_base_dir %>/conf/conf.d/*.cnf;
    include <%= @nginx_base_dir %>/conf/sites/*.conf;
}

